<?xml version="1.0" encoding="UTF-8"?>
<beans  
    xmlns="http://www.springframework.org/schema/beans"  
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"  
    xmlns:p="http://www.springframework.org/schema/p"  
    xsi:schemaLocation="http://www.springframework.org/schema/beans  
                http://www.springframework.org/schema/beans/spring-beans-3.0.xsd">  
  
<bean id="empbean" class="com.cg.model.Employee" init-method="myInitMethod" destroy-method="MyDestroyMethod">  
	<property name="id">  <value>20</value>  </property>
	<property name="name">  <value>Alisa</value>  </property>
	<property name="salary">  <value>50000.0</value>  </property>
</bean>  
 
</beans>